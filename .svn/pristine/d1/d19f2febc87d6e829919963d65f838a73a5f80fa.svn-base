package org.jclarion.clarion.appgen.template;

import org.jclarion.clarion.util.SharedReader;

public class BufferedWriteTargetReader extends SharedReader
{
	private BufferedWriteTarget target;

	public BufferedWriteTargetReader(char[] buffer, int len,BufferedWriteTarget target) 
	{
		super(buffer, len);
		this.target=target;
	}
	
	public BufferedWriteTarget getBufferedWriteTarget()
	{
		return target;
	}

}
