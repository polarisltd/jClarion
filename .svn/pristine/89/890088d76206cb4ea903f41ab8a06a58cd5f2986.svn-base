package org.jclarion.clarion.ide.windowdesigner;

import org.jclarion.clarion.PropertyObject;
import org.jclarion.clarion.constants.Prop;

public class HVScrollPropSerializer extends PropSerializer 
{
	public void deserialize(PropertyObject target,String type,String... params)
	{
		target.setProperty(Prop.HSCROLL,true);
		target.setProperty(Prop.VSCROLL,true);	
	}
}

