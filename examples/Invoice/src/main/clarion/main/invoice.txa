       PROMPT('Zipcode:'),AT(8,61),USE(?COM:Zipcode:Prompt),#ORIG(?COM:Zipcode:Prompt),#LINK(?COM:Zipcode)
       ENTRY(@K#####|-####K),AT(49,61,64,10),USE(COM:Zipcode),MSG('Company''s zipcode'),#ORIG(COM:Zipcode), |
           #LINK(?COM:Zipcode:Prompt)
       PROMPT('Phone:'),AT(8,74),USE(?COM:Phone:Prompt),#ORIG(?COM:Phone:Prompt),#LINK(?COM:Phone)
       ENTRY(@P(###) ###-####P),AT(49,74,64,10),USE(COM:Phone),MSG('Company''s phone number'),#ORIG(COM:Phone), |
           #LINK(?COM:Phone:Prompt)
       BUTTON,AT(52,92,21,18),USE(?OK),FLAT,TIP('Save changes and exit'),ICON('DISK12.ICO'),DEFAULT,#SEQ(2), |
           #ORIG(?OK)
       BUTTON,AT(88,92,21,18),USE(?Help),FLAT,TIP('Get Help'),ICON(ICON:Help),STD(STD:Help)
