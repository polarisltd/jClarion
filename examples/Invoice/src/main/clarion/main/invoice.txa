           #LINK(?COM:Address:Prompt)
       PROMPT('City:'),AT(8,34,17,10),USE(?COM:City:Prompt),#ORIG(?COM:City:Prompt),#LINK(?COM:City)
       ENTRY(@s25),AT(49,34,104,10),USE(COM:City),MSG('Company''s city'),CAP,#ORIG(COM:City),#LINK(?COM:City:Prompt)
