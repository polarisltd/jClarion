QuickWindow WINDOW('Update Company'),AT(,,199,121),FONT('MS Sans Serif',8,COLOR:Black,),CENTER,IMM,HLP('~UpdateCompany'), |
         SYSTEM,GRAY,RESIZE
       SHEET,AT(4,2,191,117),USE(?CurrentTab),WIZARD,#ORIG(CurrentTab)
         TAB('Tab 1'),USE(?Tab1),#ORIG(?Tab1)
         END
       END
       PROMPT('Name:'),AT(8,7),USE(?COM:Name:Prompt),#ORIG(?COM:Name:Prompt),#LINK(?COM:Name)
       ENTRY(@s20),AT(49,7,84,10),USE(COM:Name),MSG('Company name'),CAP,#ORIG(COM:Name),#LINK(?COM:Name:Prompt)
