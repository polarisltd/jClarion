PROPERTY:BEGIN
PRIORITY 8800
LABEL
PROPERTY:END
      !Display filename only and not the path.
      IF LOC:FileName <> ''
        LX# = LEN(CLIP(LOC:FileName))
        LOOP X# = LX# TO 1 BY -1
          IF LOC:FileName[X#] = '\'
            BREAK
          END
        END
        IF LOC:FileName[1 : (X#-1)] = LONGPATH(PATH())
          PRO:PictureFile = UPPER(LOC:FileName[(X#+1) : LX#])
          DISPLAY
        END
      END
[END]
