[PROPERTYCLASS]
  NAME: TOPSPEED:ControlTemplate#ReportProcess
  DESCRIPTOR: 1
  PROPERTY: NAME(NextButtonControl),ACORN(1),ACORN.NAME(NextButtonControl),ACORN.WIZARD(ControlTemplate#NextButton),DEVIMP(3),DTYPE(2),TYPE(7)
  PROPERTY: NAME(PreviousButtonControl),ACORN(1),ACORN.NAME(PreviousButtonControl),ACORN.WIZARD(ControlTemplate#PreviousButton),DEVIMP(3),DTYPE(2),TYPE(7)
  PROPERTY: NAME(RecordsPerPage),DEVIMP(5),DTYPE(5),OUTPUT.NAME(ReportRecsPerPage),TYPE(3)
  PROPERTY: NAME(BreakChecking),DESCRIPTION('Select the granularity of breaks within the report.'),DEVIMP(1),DTYPE(SINGLEDROP),TYPE(3),OUTPUT.NAME(BreakChecking)
  PROPERTY: NAME(BreakChecking.List),DTYPE(5),HIDDEN(1),LIST(1),MULTI(1),TYPE(3)
  PROPERTY: NAME(ReportSkeletonName),DEVIMP(5),DTYPE(5),OUTPUT.NAME(ReportSkeletonName),TYPE(3)
  PROPERTY: NAME(ReportTitle),DEVIMP(5),DOCUMENTATION(Report Window Title for this report.),DTYPE(5),OUTPUT.NAME(ReportTitle),TYPE(3)
  [DEFINITION]
    BREAKCHECKING(by line count)
    BREAKCHECKING.LIST(by record count, by line count)
    PARENTWIZARD(TOPSPEED:ControlTemplate)
    CONTROLTEMPLATE(WebProcessReport)
    GENERATEPRIORITY(200)
    NAME(TOPSPEED:ControlTemplate#ReportProcess)
    NEXTBUTTONCONTROL(1)
    PREVIOUSBUTTONCONTROL(1)
    RECORDSPERPAGE(%Scope.RecordsOnPage%)
    REPORTSKELETONNAME(%Scope.ReportSkeleton%)
    REPORTTITLE(%Scope.ReportName%)
    TEMPLATECHAIN(Web)
  [END]
[END]
