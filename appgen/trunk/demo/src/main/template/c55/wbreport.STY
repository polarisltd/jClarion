  PROPERTY: NAME(RecordsOnPage),DEVIMP(5),DTYPE(5),OUTPUT.NAME(),TYPE(3)
  PROPERTY: NAME(ReportName),DESCRIPTION(Descriptive name for this report),OUTPUT.NAME()
  PROPERTY: NAME(ReportProcess),ACORN(1),ACORN.NAME(ReportProcess),ACORN.WIZARD(ControlTemplate#ReportProcess),DEVIMP(3),DTYPE(2),HIDDEN(1),LOCKED(1),TYPE(7)
  PROPERTY: NAME(ReportSkeleton),DEVIMP(5),DTYPE(5),TYPE(3)
